{
  "version": 2,
  "builds": [{
    "src": "index.js",
    "use": "@now/node-server"
  }],
  "routes": [{
      "src": "/api/users",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/user/:id",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/logout",
      "dest": "/server.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/api/user/setAvatar",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/user",
      "dest": "/server.js",
      "methods": ["POST", "PUT", "DELETE"]
    }
  ]
}